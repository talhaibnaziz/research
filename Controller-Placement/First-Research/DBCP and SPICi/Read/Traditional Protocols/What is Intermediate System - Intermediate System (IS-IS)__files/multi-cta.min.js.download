$(document).ready(function(){$(".multi-cta").each(function(){$("ul",this).first().addClass("multi-cta-bullets-container");countBullets=$(".multi-cta-bullets-container",this).find(">li").length;if(countBullets>10){countBullets="10plus"}$(this).addClass("multi-cta-bullets-"+countBullets);$(this).addClass("bounce-transition");$("li",this).addClass("bounce-transition");$("li a",this).addClass("bounce-transition");$("li li img",this).addClass("smooth-transition")});$(".multi-cta>ul>li>a").each(function(){$(this).parent().addClass("multi-cta-item-has-link")});$("img[src*='icons']").each(function(){$(this).addClass("icon");$(this).parent().parent().parent().addClass("multi-cta-item-has-icon")});$("img[src*='Free-icons']").each(function(){$(this).addClass("black-icon")});$("img[src*='black-icons']").each(function(){$(this).addClass("black-icon")});$("img[src*='Logo']").each(function(){$(this).addClass("logo")});$("img[src*='Thumbnail']").each(function(){$(this).addClass("document-thumbnail")})});