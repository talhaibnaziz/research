            //console.log(self);
            e.preventDefault();
            //console.log("Sorting comments. Currently sort-by-oldest = " + self.sortByOldest);

            // Change the page's sort order (this will be used for displaying the newly sorted comments)
            self.sortByOldest = !self.sortByOldest;

            var $comments = self.commentList.children('.comment-parent');

            if( self.sortByOldest ) {
                window.$('#sortByOldest').show();
                window.$('#sortByNewest').hide();
            } else {
                window.$('#sortByOldest').hide();
                window.$('#sortByNewest').show();
            }

            //sort comments, aka top level
            $comments.sort(function(a,b){
                if( self.sortByOldest ) {
                    return window.$(a).attr("data-sort") - $(b).attr("data-sort");
                } else {
                    return window.$(b).attr("data-sort") - $(a).attr("data-sort");
                }
            });

            //sort replies
            $comments.each(function() {
                var p = $(this);
                p.append(p.children(".comment-child").get().sort(function(a,b) {
                    if( self.sortByOldest ) {
                        return $(a).attr("data-sort") - $(b).attr("data-sort");
                    } else {
                        return $(b).attr("data-sort") - $(a).attr("data-sort");
                    }
                }));
            });

            //re-write comments
            $comments.detach().appendTo(self.commentList);
        });

        scrollToElement();
    });
