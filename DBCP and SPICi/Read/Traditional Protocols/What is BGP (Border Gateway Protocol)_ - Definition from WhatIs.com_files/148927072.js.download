bindScrollDepth = function(documentPass, docNum){
    var startTime = +new Date(),
        cache = [];
    window.$(window).on('scroll', throttle(function() {
        var docHeight = window.$(documentPass).height(),
            documentOffset = window.$(documentPass).offset().top,
            winHeight = window.innerHeight ? window.innerHeight : window.$(window).height(),
            scrollDistance = window.$(window).scrollTop() + winHeight,
            marks = calculateMarks(docHeight, documentOffset),
            timing = +new Date() - startTime;
        checkMarks(marks, scrollDistance, timing, docNum);
    }, 500));
    function calculateMarks(docHeight, documentOffset) {
        return {
            /*'0%' : parseInt(docHeight * 0.01, 10) + documentOffset,
             '25%' : parseInt(docHeight * 0.25, 10) + documentOffset,
             '50%' : parseInt(docHeight * 0.50, 10) + documentOffset,
             '75%' : parseInt(docHeight * 0.75, 10) + documentOffset,
             '100%': docHeight - 5 + documentOffset*/
            '0prcnt' : parseInt(docHeight * 0.01, 10) + documentOffset,
            '25prcnt' : parseInt(docHeight * 0.25, 10) + documentOffset,
            '50prcnt' : parseInt(docHeight * 0.50, 10) + documentOffset,
            '75prcnt' : parseInt(docHeight * 0.75, 10) + documentOffset,
            '100prcnt': docHeight - 5 + documentOffset
        };
    }
    function checkMarks(marks, scrollDistance, timing, docNumPass) {
        $.each(marks, function(key, val) {
            if ( $.inArray(key, cache) === -1 && scrollDistance >= val ) {
                window.optimizely.push(["trackEvent", "scrollPosition_"+ docNumPass +"_" + key]);
                cache.push(key);
            }
        });
    }
    function throttle(func, wait) {
        var context, args, result;
        var timeout = null;
        var previous = 0;
        var later = function() {
            previous = new Date();
            timeout = null;
            result = func.apply(context, args);
        };
        return function() {
            var now = new Date();
            if (!previous) previous = now;
            var remaining = wait - (now - previous);
            context = this;
            args = arguments;
            if (remaining <= 0) {
                clearTimeout(timeout);
                timeout = null;
                previous = now;
                result = func.apply(context, args);
            } else if (!timeout) {
                timeout = setTimeout(later, remaining);
            }
            return result;
        };
    }
};
function timeOnPage() {
    var startTime = new Date().getTime();
    i = 1;
    var interval = setInterval(function(){
        if(new Date().getTime() - startTime > 6000){
            clearInterval(interval);
            return;
        }
        window.optimizely.push(["trackEvent", "timeOnPage_" + i + "_second"]);
        i++;
    }, 1000);
}
