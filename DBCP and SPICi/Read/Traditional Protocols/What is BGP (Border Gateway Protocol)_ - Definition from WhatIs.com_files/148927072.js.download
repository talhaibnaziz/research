function scrollToElement() {
    window.$(".main-article-comment-count").mousedown(function (e) {
        //console.log("cg - scroll to element");
        var clickElement = e.target;
        var parent = window.$(clickElement).closest(".main-content");
        var parentId = window.$(clickElement).closest(".main-content").attr("id");
        var targetElement = window.$(parent).find("#commenting");
        var commentsY = window.$(targetElement).offset();
        commentsY = commentsY.top;
        //console.log("cg - parentId: "+parentId);
        if (parentId === "main-content-0") {
            //console.log("cg - in #main-content-0");
            window.$.scrollTo(commentsY - 300, 300);
        }
        else {
            window.$.scrollTo(commentsY - 60, 300);
        }
    });
}
function commentsCollapseToggle(collapseComments) {
    var $commentsToggle = $(collapseComments);
    var $commentsSection = $(collapseComments).parents('.comment-wrapper').parent('div');
    var $commentsChildrenNum = $commentsSection.find('.comment-child').length;

    if ($commentsSection.hasClass('closed')) {
        $commentsToggle.text('[-]');
        $commentsSection.removeClass('closed');
        $commentsSection.find('.comment-user-info .comment-num').remove();
    }
    else {
        $commentsToggle.text('[+]');
        $commentsSection.addClass('closed');
        $commentsSection.find('.comment-user-info').append('<span class=comment-num>' + '(' + $commentsChildrenNum + ' replies' + ')');
    }
};

