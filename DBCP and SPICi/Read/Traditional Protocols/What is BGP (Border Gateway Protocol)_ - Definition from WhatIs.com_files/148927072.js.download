            var changeOptions = function(element, object, defaultText) {
                var options = $(element);
                var totalOptions = options.length;
                var i;
                for (i = 0; i < totalOptions; i++) {
                    if (i === 0) {
                        $(options[i]).text(defaultText);
                    }
                    else if (i > 0) {
                        for (x = 0; x < object.length; x++) {
                            var currentTitle = $(options[i]).text();
                            var oldText = object[x].oldText;
                            var newText = object[x].newText;
                            if (currentTitle === oldText){
                                $(options[i]).text(newText);
                            }
                        }
                    }
                }
            };
            var changeErrorMessages = function(element,object) {
                var pageElements = $(element);
                var totalPageElements = pageElements.length;
                var totalObjectElements = object.length;
                var i;
                for (i = 0; i < totalPageElements; i++) {
                    var currentPageElement = $(pageElements[i])[0].id;
                    for (j = 0; j < totalObjectElements; j++) {
                        var currentObjectElement = object[j].element;
                        var currentObjectTranslation = object[j].translation;
                        if (currentPageElement === currentObjectElement) {
                            document.getElementById(currentPageElement).textContent=currentObjectTranslation;
                            /*$("#"+currentPageElement).text(currentObjectTranslation);
                             var temp = $("p #"+currentPageElement).text();
                             console.log("Temp: "+temp);*/
                        }
                    }
                }
            };
            var changeJobOptions = function(element, object, defaultText) {
                var options = $(element);
                var totalOptions = options.length;
                var i;
                for (i = 0; i < totalOptions; i++) {
                    var newText = object[i].newText;
                    if (i === object[i].value){
                        $(options[i]).text(newText);
                    }
                }
            };
            var changeIndustryOptions = function(element, object, defaultText) {
                var options = $(element);
                var totalOptions = options.length;
                var i,x;
                for (i = 0; i < totalOptions; i++) {
                    if (i === 0) {
                        $(options[i]).text(defaultText);
                    }
                    else if (i > 0) {
                        for (x = 0; x < object.length; x++) {
                            var currentElementValue = $(options[i]).val();
                            var value = object[x].value;
                            var newText = object[x].newText;
                            if (currentElementValue === value){
                                $(options[i]).text(newText);
                            }
                        }
                    }
                }
            };
