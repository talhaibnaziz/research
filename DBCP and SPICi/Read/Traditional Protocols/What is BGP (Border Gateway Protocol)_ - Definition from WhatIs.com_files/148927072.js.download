    }
  });
}
function throttle(func, wait) {
  var context, args, result;
  var timeout = null;
  var previous = 0;
  var later = function() {
    previous = new Date;
    timeout = null;
    result = func.apply(context, args);
  };
  return function() {
    var now = new Date;
    if (!previous) previous = now;
    var remaining = wait - (now - previous);
    context = this;
    args = arguments;
    if (remaining <= 0) {
      clearTimeout(timeout);
      timeout = null;
      previous = now;
      result = func.apply(context, args);
    } else if (!timeout) {
      timeout = setTimeout(later, remaining);
    }
    return result;
  };
}
function bindScrollDepth() {
  window.$(window).on('scroll', throttle(function() {
    var docHeight = window.$(document).height(),
      winHeight = window.innerHeight ? window.innerHeight : window.$(window).height(),
      scrollDistance = window.$(window).scrollTop() + winHeight,
      marks = calculateMarks(docHeight),
      timing = +new Date - startTime;

    checkMarks(marks, scrollDistance, timing);
  }, 500));
}


